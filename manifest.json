{
  "manifest_version": 2,

  "name": "Novovu Manager",
  "description": "Created by MixeroTN#9156",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "js": [
        "VisionModifier.js"
      ],
      "matches": [
        "*://www.novovu.com/*"
      ]
    }
  ],
  "permissions": ["tabs", "*://www.novovu.com/*"]
}
